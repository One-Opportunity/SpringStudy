<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Map">
	<insert id="insertData" parameterType="kr.co.abcde.boardMap.dto.MapDTO">
		INSERT INTO TB_BOARD_MAP (
			MAP_ID
			,MAP_NAME
			,MAP_SORT
			,MAP_TYPE
			,PAR_MAP_ID
		) VALUES (
			#{mapId}
			,#{mapName}
			,#{mapSort}
			,#{mapType}
			,#{parMapId}
		)
	</insert>
	
	<select id="selectOne" parameterType="int" resultType="kr.co.abcde.boardMap.dto.MapDTO">
		SELECT *
		FROM TB_BOARD_MAP
		WHERE MAP_ID = #{mapId}
	</select>
</mapper>